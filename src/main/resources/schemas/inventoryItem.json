{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "InventoryItem",
  "description": "A single CCE Inventory item",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "facility": {
      "type": "object",
      "$ref": "basicFacilityDto.json",
      "title": "facility"
    },
    "catalogItem": {
      "type": "object",
      "$ref": "catalogItem.json",
      "title": "catalogItem"
    },
    "programId": {
      "type": "string",
      "title": "programId"
    },
    "equipmentTrackingId": {
      "type": "string",
      "title": "equipmentTrackingId"
    },
    "referenceName": {
      "type": "string",
      "title": "referenceName"
    },
    "yearOfInstallation": {
      "type": "integer",
      "title": "yearOfInstallation"
    },
    "yearOfWarrantyExpiry": {
      "type": "integer",
      "title": "yearOfWarrantyExpiry"
    },
    "source": {
      "type": "string",
      "title": "source"
    },
    "functionalStatus": {
      "enum": [
        "FUNCTIONING",
        "NON_FUNCTIONING",
        "OBSOLETE"
      ],
      "title": "functionalStatus"
    },
    "reasonNotWorkingOrNotInUse": {
      "enum": [
        "AWAITING_REPAIR",
        "NEEDS_SPARE_PARTS",
        "UNSERVICEABLE",
        "NO_FINANCE",
        "NO_FUEL",
        "SURPLUS",
        "DEAD",
        "NOT_APPLICABLE"
      ],
      "title": "reasonNotWorkingOrNotInUse"
    },
    "utilization": {
      "enum": [
        "ACTIVE",
        "NOT_IN_USE",
        "IN_STORE_FOR_ALLOCATION"
      ],
      "title": "utilization"
    },
    "voltageStabilizer": {
      "enum": [
        "YES",
        "NO",
        "UNKNOWN",
        "NOT_APPLICABLE"
      ],
      "title": "voltageStabilizer"
    },
    "backupGenerator": {
      "enum": [
        "YES",
        "NO",
        "UNKNOWN",
        "NOT_APPLICABLE"
      ],
      "title": "backupGenerator"
    },
    "voltageRegulator": {
      "enum": [
        "YES",
        "NO",
        "UNKNOWN",
        "NOT_APPLICABLE",
        "NA"
      ],
      "title": "voltageRegulator"
    },
    "manualTemperatureGauge": {
      "enum": [
        "BUILD_IN",
        "PAIRED",
        "NO_GAUGE"
      ],
      "title": "manualTemperatureGauge"
    },
    "remoteTemperatureMonitor": {
      "enum": [
        "BUILD_IN",
        "PAIRED",
        "NO_RTM"
      ],
      "title": "remoteTemperatureMonitor"
    },
    "remoteTemperatureMonitorId": {
      "type": "string",
      "title": "remoteTemperatureMonitorId"
    },
    "additionalNotes": {
      "type": "string",
      "title": "additionalNotes"
    },
    "decommissionDate": {
      "type": ["string", "null"],
      "title": "decommissionDate"
    },
    "modifiedDate": {
      "type": ["string", "null"],
      "title": "modifiedDate"
    },
    "lastModifier": {
      "type": ["object", "null"],
      "title": "lastModifier"
    }
  },
  "required": [
    "catalogItem",
    "facility",
    "programId",
    "yearOfInstallation",
    "functionalStatus",
    "referenceName",
    "utilization",
    "voltageStabilizer",
    "backupGenerator",
    "voltageRegulator",
    "manualTemperatureGauge",
    "remoteTemperatureMonitor"
  ]
}
